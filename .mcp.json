{
  "mcpServers": {
    "netlify": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@netlify/mcp"
      ],
      "env": {}
    },
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp"
    },
    "shadcn": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ],
      "env": {}
    }
  }
}