{
  "name": "startupai-site",
  "version": "1.0.0",
  "description": "Professional consulting website for Chris Walker - eCommerce Strategy & Development",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:local": "next dev --turbopack -p 3000",
    "dev:staging": "netlify dev",
    "build": "next build",
    "build:staging": "NODE_ENV=staging next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,html,css}\"",
    "env:check": "node -e \"console.log('Environment:', process.env.NODE_ENV || 'development'); console.log('Marketing URL:', process.env.NEXT_PUBLIC_MARKETING_URL); console.log('App URL:', process.env.NEXT_PUBLIC_APP_URL);\""
  },
  "keywords": [
    "ecommerce",
    "consulting",
    "development",
    "strategy",
    "startup"
  ],
  "author": "Chris Walker",
  "license": "MIT",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fontsource/inter": "^4.5.14",
    "@formspree/react": "^3.0.0",
    "@hookform/resolvers": "^5.2.1",
    "@netlify/functions": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "eslint": "^8.38.0",
    "eslint-config-next": "^15.5.3",
    "eslint-config-prettier": "^8.8.0",
    "lucide-react": "^0.539.0",
    "next": "^15.5.3",
    "postcss": "^8.4.21",
    "posthog-js": "^1.270.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "prettier": "^3.0.0",
    "supabase": "^2.47.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
