{
  "name": "startupai-site",
  "version": "1.0.0",
  "description": "Professional consulting website for Chris Walker - eCommerce Strategy & Development",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:local": "next dev --turbopack -p 3000",
    "dev:staging": "netlify dev",
    "build": "next build",
    "build:staging": "NODE_ENV=staging next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,html,css}\"",
    "env:check": "node -e \"console.log('Environment:', process.env.NODE_ENV || 'development'); console.log('Marketing URL:', process.env.NEXT_PUBLIC_MARKETING_URL); console.log('App URL:', process.env.NEXT_PUBLIC_APP_URL);\""
  },
  "keywords": [
    "ecommerce",
    "consulting",
    "development",
    "strategy",
    "startup"
  ],
  "author": "Chris Walker",
  "license": "MIT",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fontsource/inter": "^4.5.15",
    "@formspree/react": "^3.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@netlify/functions": "^5.1.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stripe/react-stripe-js": "^5.4.0",
    "@stripe/stripe-js": "^8.5.2",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.81.1",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.9",
    "eslint-config-prettier": "^8.10.2",
    "lucide-react": "^0.539.0",
    "next": "^15.5.9",
    "postcss": "^8.5.6",
    "posthog-js": "^1.293.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "stripe": "^20.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "prettier": "^3.6.2",
    "supabase": "^2.58.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
